{
  "name": "s2m-support-server",
  "version": "1.0.0",
  "description": "Express app for hosting webapp",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "seed": "ts-node src/seeds/seedScript.ts",
    "dev": "nodemon --watch src --watch .env --exec ts-node src/index.ts",
    "build": "tsc && copyfiles -u 2 \"src/data/geo/**/*\" dist/data && copyfiles -u 1 \"src/generated/**/*\" dist/",
    "build:onprem": "webpack --config webpack.config.js",
    "build:exe": "node buildBin.js",
    "prod": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "npx prettier --write ./src",
    "test": "jest"
  },
  "prisma": {
    "seed": "ts-node src/seeds/seedScript.ts"
  },
  "pkg": {
    "scripts": [
      "singlebuild/index.js"
    ],
    "assets": [
      "static/**/*",
      "src/generated/client/query_engine-windows.dll.node"
    ],
    "files": [
      "**/*",
      "!**/*.env"
    ],
    "outputpath": "build"
  },
  "author": "",
  "license": "ISC",
